Mode = switch
AddressFamily = any
Interface = __TAP_ADAPTER_NAME__
DeviceType = tap
BroadcastSubnet = __VPN_NETWORK__.255
UPnP = yes
LocalDiscovery = yes
LogLevel = 1

#we want low latency
ProcessPriority = high

#Ed25519PrivateKeyFile=keys/xxx.key # private key is set by start-tinc.cmd
#BindToAddress=* 655 # port number is set by start-tinc.cmd

#Setting this to zero will disable replay tracking completely and pass all traffic, but leaves tinc vulnerable to replay-based attacks on your traffic.
ReplayWindow = 0
